<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="XG.Model" namespace="XG.Model.Domain">

	<class name="Bot">
		<id name="Guid">
			<generator class="assigned"/>
		</id>
		<property name="Name" />
		<property name="Connected" />
		<property name="Enabled" />
		<property name="State" />
		<property name="LastMessage" />
		<property name="LastMessageTime" />
		<property name="LastContact" />
		<property name="QueuePosition" />
		<property name="QueueTime" />
		<property name="InfoSpeedMax" />
		<property name="InfoSpeedCurrent" />
		<property name="InfoSlotTotal" />
		<property name="InfoSlotCurrent" />
		<property name="InfoQueueTotal" />
		<property name="InfoQueueCurrent" />
		<property name="HasNetworkProblems" />
		<many-to-one name="Parent" column="ParentGuid" not-null="true" />
		<bag name="Children" inverse="true" cascade="all-delete-orphan" lazy="false" mutable="true">
			<key foreign-key="FK_PACKET_BOT_ID">
				<column name="ParentGuid" />
			</key>
			<one-to-many class="Packet" />
		</bag>
	</class>

</hibernate-mapping>
