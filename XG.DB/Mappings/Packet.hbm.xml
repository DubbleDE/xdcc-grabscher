<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="XG.Model" namespace="XG.Model.Domain">

	<class name="Packet">
		<id name="Guid">
			<generator class="assigned"/>
		</id>
		<property name="Name" />
		<property name="Connected" />
		<property name="Enabled" />
		<property name="Size" />
		<property name="RealSize" />
		<property name="RealName" />
		<property name="LastUpdated" />
		<property name="LastMentioned" />
		<many-to-one name="Parent" column="ParentGuid" not-null="true" />
	</class>

</hibernate-mapping>
